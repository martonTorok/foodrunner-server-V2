    "@types/bcryptjs": "^2.4.2",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^6.1.0",
    "@types/expect": "^1.20.3",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.5",
    "@types/supertest": "^2.0.7",
